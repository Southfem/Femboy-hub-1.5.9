-- MINI BUTTONS FOR HALF HUB
local function createButton(data, index)
    local btn = Instance.new("TextButton")
    btn.Name = data.name
    btn.Parent = ScrollingFrame
    btn.BackgroundColor3 = Color3.fromRGB(255, 230, 238)
    btn.BorderSizePixel = 0
    btn.Size = UDim2.new(1, -16, 0, 28)  -- EVEN SMALLER!
    btn.Font = Enum.Font.GothamSemibold
    btn.Text = data.name
    btn.TextColor3 = Color3.fromRGB(255, 105, 180)
    btn.TextSize = 12  -- TINY TEXT
    btn.TextWrapped = true
    btn.LayoutOrder = index
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 6)  -- Smaller corners
    corner.Parent = btn
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 105, 180), TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 230, 238), TextColor3 = Color3.fromRGB(255, 105, 180)}):Play()
    end)
    
    btn.MouseButton1Click:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.1), {Size = UDim2.new(1, -16, 0, 25)}):Play()
        wait(0.1)
        TweenService:Create(btn, TweenInfo.new(0.1), {Size = UDim2.new(1, -16, 0, 28)}):Play()
        
        StarterGui:SetCore("SendNotification", {
            Title = "Loading...";
            Text = data.name .. " loading...";
            Duration = 2;
        })
        
        pcall(function()
            loadstring(game:HttpGet(data.url))()
        end)
        
        StarterGui:SetCore("SendNotification", {
            Title = data.name;
            Text = "Loaded!! ðŸ’–";
            Duration = 2;
        })
    end)
end

